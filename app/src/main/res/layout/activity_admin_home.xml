<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Admin.AdminHome">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="399dp"
        android:layout_height="224dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.012"
        app:srcCompat="@drawable/logo" />


    <Button
        android:id="@+id/add_bin"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="Add Bin Location"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.372" />

    <Button
        android:id="@+id/view_bin"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="View Bin Location"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.471" />

    <Button
        android:id="@+id/del_bin"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="Delete Bin Location"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566" />

    <Button
        android:id="@+id/view_comp"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="View Complaint"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.668" />

    <Button
        android:id="@+id/view_feed"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="View Feedback"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.763" />

    <Button
        android:id="@+id/delete_account1"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="Delete Account"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.512"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.861" />

    <Button
        android:id="@+id/logout1"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/aclonica"
        android:gravity="center"
        android:paddingBottom="8dp"
        android:text="Logout"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

</androidx.constraintlayout.widget.ConstraintLayout>